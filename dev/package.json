    "@payloadcms/storage-s3": "3",
    "@payloadcms/ui": "3",
    "next": "15.0.3",
    "payload": "3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "sharp": "^0.34.1"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "typescript": "5.5.3"
